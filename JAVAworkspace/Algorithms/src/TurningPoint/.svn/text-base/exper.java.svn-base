package TurningPoint;

public class exper {
	
	

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		double[] ary1 = {2, 4, 5, 3, 6};
		double[] ary2 = {5, 3, 7, 8, 1};
		Vector vec1 = new Vector(ary1);
		System.out.println(vec1);
		Vector vec2 = new Vector(ary2);
		System.out.println(vec2);
		System.out.println(vec1.dot(vec2));
		System.out.println(vec1.magnitude());
		System.out.println(vec1.direction());
		System.out.println(vec1.times(20.8));
		System.out.println(vec1.length());
		System.out.println(vec1.distanceTo(vec2));
		
		Vector vec3 = vec1.plus(vec2);
		System.out.println(vec3);
		
		
	}

}
